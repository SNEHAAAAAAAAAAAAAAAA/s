def bignum(a,b):
	if a>b:
		print('a is big')
	else:
		print('b is big')
a=int(input())
b=int(input())
bignum(a,b)
#way2
def bignum(a,b):
	if a>b:
		return 'a is big'
	else:
		return 'b is big'
a=int(input())
b=int(input())
print(bignum(a,b))
#way3
def bignum(a,b):
	if a>b:
		return True
	else:
		return False
a=int(input())
b=int(input())
if(bignum(a,b)):
	print('a is big')
else:
	print('b is big')
